                                <img src="/images/<%= products[i].image %>" class="product_img">
                                <div class="main-article">
                                    <p class="price"><svg class="Price--eth-icon"  xmlns="http://www.w3.org/2000/svg" width="33" height="53" viewBox="0 0 33 53" fill="none">
                                        <path d="M16.3576 0.666687L16.0095 1.85009V36.1896L16.3576 36.5371L32.2976 27.115L16.3576 0.666687Z" fill="#343434"/>
