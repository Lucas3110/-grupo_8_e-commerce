                                        <path d="M16.3575 36.537L32.2973 27.1151L16.3575 19.8699V36.537Z" fill="#141414"/>
                                        <path d="M0.417816 27.1151L16.3576 36.537V19.8699L0.417816 27.1151Z" fill="#393939"/>
                                        </svg>
                                        <%= products[i].price %>
                                    </p>
                                    <h4 class="description">
                                        <%= products[i].name %>
